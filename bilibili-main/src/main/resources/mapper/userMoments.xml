<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.juneqqq.dao.UserMomentsDao">

    <insert id="addUserMoments" parameterType="com.juneqqq.entity.dao.UserMoment" useGeneratedKeys="true" keyProperty="id">
        insert into
            t_user_moments(
                userId,
                `type`,
                contentId
            )values(
                #{userId},
                #{type},
                #{contentId},
            )
    </insert>

</mapper>