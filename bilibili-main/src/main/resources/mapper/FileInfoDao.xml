<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.juneqqq.dao.FileInfoDao">

    <insert id="addFile" parameterType="com.juneqqq.entity.dao.FileInfo">
        insert into
            t_file_info(
                url,
                `suffix`,
                md5,
                createTime
            )value(
                #{url},
                #{suffix},
                #{md5},
                #{createTime}
            )
    </insert>

    <select id="getFileByMD5" parameterType="java.lang.String" resultType="com.juneqqq.entity.dao.FileInfo">
        select
            *
        from
            t_file_info
        where
            md5 = #{md5}
    </select>

</mapper>